    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "foundation-sites": "^6.3.1",
    "image-webpack-loader": "^3.3.1",
    "lazy-route": "^1.0.7",
    "lodash": "^4.17.4",
