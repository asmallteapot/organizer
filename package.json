  "version": "0.0.1",
  "scripts": {
    "postinstall": "webpack -p --config webpack.prod.js",
    "build": "webpack -p --config webpack.prod.js",
    "server": "webpack-dev-server --config webpack.dev.js",
    "start": "concurrently -p name --names \"WEBPACK,DJANGO\" --kill-others \"npm run server\" \"python manage.py runserver\" "
