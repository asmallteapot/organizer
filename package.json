    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "syntax-dynamic-import",
      "babel-plugin-lodash",
      "transform-object-rest-spread",
      "react-hot-loader/babel"
    ]
